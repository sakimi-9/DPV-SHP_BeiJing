{
    "name": "rendering",
    "module": "src/main.ts",
    "type": "module",
    "scripts": {
        "dev": "bun vite",
        "build": "bun vite build",
        "preview": "bun vite preview",
        "docker:build": "docker build -t sakimi9/dpv-shp_beijing:latest .",
